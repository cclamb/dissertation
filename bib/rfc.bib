@TECHREPORT{rfc1513,
AUTHOR="S. Waldbusser",
TITLE="Token Ring Extensions to the Remote Network Monitoring {MIB}",
TYPE="RFC",
INSTITUTION="Internet Engineering Task Force",
NUMBER=1513,
PAGES=55,
DAYS=15,
MONTH=sep,
YEAR=1993,
ABSTRACT="This memo defines extensions to the Remote Network Monitoring
