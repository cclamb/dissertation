@TECHREPORT{rfc3301,
AUTHOR="Y. T'Joens and P. Crivellari and B. Sales",
TITLE="Layer Two Tunnelling Protocol {(L2TP):} {ATM} access network extensions",
TYPE="RFC",
INSTITUTION="Internet Engineering Task Force",
