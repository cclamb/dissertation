}

@TECHREPORT{rfc3355,
AUTHOR="A. Singh and R. Turner and R. Tio and S. Nanji",
TITLE="Layer Two Tunnelling Protocol {(L2TP)} Over {ATM} Adaptation Layer 5
{(AAL5)}",
