@TECHREPORT{rfc2559,
AUTHOR="S. Boeyen and T. Howes and P. Richard",
TITLE="Internet {X.509} Public Key Infrastructure Operational Protocols - {LDAPv2}",
TYPE="RFC",
INSTITUTION="Internet Engineering Task Force",
