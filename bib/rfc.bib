@TECHREPORT{rfc1755,
AUTHOR="M. {P{\'{e}}rez} and F. Liaw and Allison Mankin and E. Hoffman and D.
Grossman and A. Malis",
TITLE="{ATM} Signaling Support for {IP} over {ATM}",
TYPE="RFC",
